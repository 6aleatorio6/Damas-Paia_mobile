{
  "expo": {
    "name": "Damas Paia",
    "slug": "Damas-Paia",
    "version": "2.0.0",
    "scheme": "damas-paia",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#1E1E1E"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.damaspaia"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png",
        "backgroundColor": "#1E1E1E"
      },
      "package": "com.damaspaia"
    },
    "web": {
      "favicon": "./src/assets/icon.png"
    },
    "experiments": {
      "typedRoutes": true
    },
    "plugins": ["expo-router"]
  }
}
